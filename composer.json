{
    "name": "kiwi/contao-responsive-base",
    "type": "contao-bundle",
    "keywords": ["contao", "grid", "responsive"],
    "description": "This bundle enables you to place contents within a self configured responsive grid system within Contao CMS.",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Katrin Zimmermann",
            "homepage": "https://github.com/diekatrin"
        },
        {
            "name": "KIWI. Werbeagentur GmbH",
            "homepage": "https://www.kiwi.de"
        }
    ],
    "require": {
        "php": "^8.4",
        "contao/core-bundle": "~5.3.13",
        "kiwi/contao-cmx": "^1.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4" : {
            "Kiwi\\Contao\\ResponsiveBaseBundle\\" : "src/"
        }
    },
    "extra":{
        "contao-manager-plugin": "Kiwi\\Contao\\ResponsiveBaseBundle\\ContaoManager\\Plugin"
    }
}